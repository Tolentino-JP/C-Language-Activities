//Asterisk triangle print
#include <stdio.h>

int main() {
    
    int i, j;

    for(i=4;i>=0;i--){
        for(j=0;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
   for(i=0;i<=4;i++){
       for(j=i;j<4;j++){
           printf(" ");
       }
       for(j=0;j<=i;j++){
           printf("*");
       }//for2
       printf("\n");
   }//for1    
    return 0;
}
